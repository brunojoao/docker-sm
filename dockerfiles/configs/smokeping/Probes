*** Probes ***

+ FPing
binary = /usr/sbin/fping
timeout = 5
step = 120

+ FPing6
binary = /usr/sbin/fping
protocol = 6
timeout = 5
step = 120

+ DNS
binary = /usr/bin/dig
lookup = google.com
pings = 5
step = 120
timeout = 5

+ TCPPing
binary = /usr/bin/tcpping
forks = 10
offset = random
pings = 5
port = 80
timeout = 5
step = 120

+ Curl
binary = /usr/bin/curl
step = 120
urlformat = http://%host/
timeout = 5
